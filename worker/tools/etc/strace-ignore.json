{
    "write": {
        "common": [
            "/dev/null",
            "/opt/src/*",
            "/proc/self/*"
        ],
        "pypi": [
            "/root/.cache/pip/*",
            "/tmp/pip-req-tracker-*",
            "/tmp/pip-unpack-*/*",
            "/tmp/pip-build-temp",
            "/tmp/pip-*/*",
            "/usr/local/lib/python*/*",
            "/usr/share/python-wheels/*",
            "/root/.cache/pip"
        ],
        "npm": [
            "/root/.npm/*",
            "/root/.config/configstore/update-notifier-npm.json*"
        ],
        "nuget": [
            "/root/.nuget/*",
            "/tmp/nuget-build-temp/*",
            "/root/.dotnet/*",
            "/root/.local/share/NuGet/*",
            "/tmp/NuGetScratch/*",
            "/tmp/CASESENSITIVETEST*",
            "/tmp/clr-debug*"
        ]
    },
    "read": {
        "common": [
            "/dev/shm/*",
            "/dev/urandom",
            "/etc/gai.conf",
            "/etc/host.conf",
            "/etc/hosts",
            "/etc/ld.so.cache",
            "/etc/nsswitch.conf",
            "/etc/os-release",
            "/etc/resolv.conf",
            "/etc/ssl/openssl.cnf",
            "/etc/ssl/certs/ca-certificates.crt",
            "/lib/*",
            "/lib/tls/*",
            "/opt/src/",
            "/proc/cpuinfo",
            "/proc/meminfo",
            "/proc/stat",
            "/proc/sys/net/*",
            "/proc/sys/vm/overcommit_memory",
            "/sys/fs/cgroup/cpu/cpu.cfs_quote_us",
            "/sys/devices/system/cpu/online",
            "/sys/fs/cgroup/memory/memory.limit_in_bytes",
            "/usr/bin/lsb_release",
            "/usr/share/zoneinfo/*",
            "/usr/share/zoneinfo-icu/*",
            "/usr/lib/*"
        ],
        "nuget": [
            "/usr/share/dotnet/*"
        ],
        "npm": [
            "/usr/lib/node_modules/*",
            "/root/.npmrc",
            "/usr/etc/npmrc"
        ],
        "pypi": [
            "/usr/bin/pip3",
            "/usr/bin/pyvenv.cfg",
            "/tmp/pip-unpack-*",
            "/usr/bin/python3",
            "/usr/pyvenv.cfg"
        ]
    }
}
